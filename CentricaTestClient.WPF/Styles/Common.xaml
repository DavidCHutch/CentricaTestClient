<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CentricaTestClient.WPF.Styles">
    <!--Colors-->
    <Color x:Key="SnowStorm1">#D8DEE9</Color>
    <Color x:Key="SnowStorm2">#E5E9F0</Color>
    <Color x:Key="SnowStorm3">#ECEFF4</Color>
    <Color x:Key="PolarNight1">#2E3440</Color>
    <Color x:Key="PolarNight2">#3B4252</Color>
    <Color x:Key="PolarNight3">#434C5E</Color>
    <Color x:Key="PolarNight4">#4C566A</Color>
    
    <!--Brushes-->
    <SolidColorBrush x:Key="BrushSnowStorm1" Color="{StaticResource SnowStorm1}"/>
    <SolidColorBrush x:Key="BrushSnowStorm2" Color="{StaticResource SnowStorm2}"/>
    <SolidColorBrush x:Key="BrushSnowStorm3" Color="{StaticResource SnowStorm3}"/>
    <SolidColorBrush x:Key="BrushPolarNight1" Color="{StaticResource PolarNight1}"/>
    <SolidColorBrush x:Key="BrushPolarNight2" Color="{StaticResource PolarNight2}"/>
    <SolidColorBrush x:Key="BrushPolarNight3" Color="{StaticResource PolarNight3}"/>
    <SolidColorBrush x:Key="BrushPolarNight4" Color="{StaticResource PolarNight4}"/>
    
    <!--Fonts-->
    
    
    
</ResourceDictionary>